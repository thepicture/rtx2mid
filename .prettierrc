{
    "printWidth": 80,
    "tabWidth": 4,
    "trailingComma": "all",
    "singleQuote": true,
    "jsxBracketSameLine": true,
    "semi": true,
    "importOrder": [
        "^app/(.*)$",
        "^entities/(.*)$",
        "^features/(.*)$",
        "^pages/(.*)$",
        "^shared/(.*)$",
        "^widgets/(.*)$",
        "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
}